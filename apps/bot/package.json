{
  "name": "@/bot",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "main": "dist/src/main/clusters.js",
  "scripts": {
    "prepare": "tsc",
    "preinstall": "tsc",
    "tsc": "tsc --noEmit --watch",
    "start": "dotenv -e ../../.env -- node .",
    "dev": "nodemon",
    "lint": "eslint ./**/*.ts",
    "interactions/create": "dotenv -e ../../.env -- node tools/createCommands.ts"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "custom"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "./src"
    ],
    "exec": "npm start",
    "ext": "ts, js"
  },
  "dependencies": {
    "@/discordeno-helpers": "workspace:*",
    "@/env": "workspace:*",
    "@discordeno/bot": "19.0.0-next.d81b28a",
    "dd-cache-proxy": "^2.0.1",
    "discord-cross-hosting": "^2.3.2",
    "discord-hybrid-sharding": "^2.1.4",
    "express": "^4.18.2",
    "nodemon": "^3.0.2"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.3.3"
  }
}
